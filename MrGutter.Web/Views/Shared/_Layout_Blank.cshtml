    <link rel="icon" type="image/x-icon" href="../../theme_assets/img/favicon/favicon.ico" />
    <link rel="stylesheet" href="../../theme_assets/vendor/libs/spinkit/spinkit.css">
    <!-- Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
